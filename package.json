{
  "name": "vue_aws",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "supabase:init": "supabase init --force",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref pjnyohdelrijdrxytdxm",
    "db:migrate:new": "supabase migration new registros",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js",
    "supabase:types": "npx supabase gen types typescript --project-id pjnyohdelrijdrxytdxm --schema public > database/types.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/vue-table": "^8.21.3",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.552.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.19.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-oxlint": "~1.23.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.23.0",
    "prettier": "3.6.2",
    "supabase": "^2.53.6",
    "typescript": "~5.9.0",
    "unplugin-vue-router": "^0.16.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vue-tsc": "^3.1.1"
  }
}
